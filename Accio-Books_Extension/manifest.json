{
    "manifest_version": 3,
    "name": "Accio-Books Search",
    "version": "1.0",
    "description": "Search Accio-Books.com for highlighted book titles.",
    "permissions": [
        "contextMenus",
        "activeTab"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}